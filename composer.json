{
    "name": "igrejanet/laravel-firebird",
    "description": "Firebird package for use with Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Matheus Lopes Santos",
            "email": "fale_com_lopez@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Igrejanet\\Firebird\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Igrejanet\\Firebird\\Providers\\FirebirdServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=7.2.5",
        "ext-pdo": "*",
        "ext-pdo_firebird": "*",
        "illuminate/database": "^7.0|^8.0",
        "harrygulliford/laravel-firebird": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "fzaninotto/faker": "^1.9",
        "illuminate/config": "^7.0|^8.0"
    }
}
